  "version": "1.0.0",
  "description": "Estudos Node",
  "main": "funcionarios.js", // ARQUIVO PRINCIPAL
  "scripts": {
    "start": "nodemon",
    "dev": "nodemon",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
